myVar = "Hello World"

newVar = 24
# int(newVar)

print(myVar + str(newVar))

# Every data type can be type casted using a function:
""" 
int() 
bool()
str()
list()
dict()
set()
float()
tuple() 
"""
# Strings are immutable objects
myVar = ["my", "list"]
print(id(myVar))
myVar.append("new stuff")
print(myVar)
print(id(myVar))

# change the given list to keep the memory address the same


myVar = ["Brand", "new", "list"]
print(id(myVar))

# to change the memory address of a list, create a new list. 
